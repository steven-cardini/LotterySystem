<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane prefHeight="480.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
   <children>
      <MenuBar maxWidth="1.7976931348623157E308" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="%file">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox prefHeight="480.0" prefWidth="600.0">
         <children>
            <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="%buy_tickets_tab">
                  <content>
                    <AnchorPane>
                           <children>
                              <VBox prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" prefHeight="30.0">
                                       <children>
                                          <Label prefWidth="160.0" text="%next_drawing_date" />
                                          <Label fx:id="labelNextDrawingDate" prefWidth="250.0" text="Fri, May 30th 2014">
                                             <HBox.margin>
                                                <Insets />
                                             </HBox.margin>
                                          </Label>
                                          <Label fx:id="langEN" alignment="CENTER_RIGHT" contentDisplay="RIGHT" onMouseClicked="#langSwitchEnglish" text="EN">
                                             <HBox.margin>
                                                <Insets left="100.0" />
                                             </HBox.margin>
                                          </Label>
                                          <Separator orientation="VERTICAL" prefHeight="10.0">
                                             <HBox.margin>
                                                <Insets left="5.0" />
                                             </HBox.margin>
                                          </Separator>
                                          <Label fx:id="langDE" onMouseClicked="#langSwitchGerman" text="DE">
                                             <HBox.margin>
                                                <Insets left="5.0" />
                                             </HBox.margin>
                                          </Label>
                                       </children>
                                       <VBox.margin>
                                          <Insets top="25.0" />
                                       </VBox.margin>
                                    </HBox>
                                    <Pane prefHeight="30.0">
                                       <children>
                                          <Label alignment="BASELINE_LEFT" text="%enter_main_numbers">
                                             <font>
                                                <Font name="System Italic" size="12.0" />
                                             </font>
                                             <padding>
                                                <Insets left="100.0" top="15.0" />
                                             </padding>
                                          </Label>
                                       </children>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </Pane>
                                    <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
                                       <children>
                                          <Label text="%main_numbers" />
                                          <TextField fx:id="fieldMainNumber0" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <TextField fx:id="fieldMainNumber1" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <TextField fx:id="fieldMainNumber2" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <TextField fx:id="fieldMainNumber3" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <TextField fx:id="fieldMainNumber4" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </TextField>
                                       </children>
                                    </HBox>
                                    <Pane prefHeight="30.0">
                                       <children>
                                          <Label alignment="BASELINE_LEFT" text="%enter_star_numbers">
                                             <font>
                                                <Font name="System Italic" size="12.0" />
                                             </font>
                                             <padding>
                                                <Insets left="100.0" top="15.0" />
                                             </padding>
                                          </Label>
                                       </children>
                                    </Pane>
                                    <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
                                       <children>
                                          <Label text="%star_numbers" />
                                          <TextField fx:id="fieldStarNumber0" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="25.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <TextField fx:id="fieldStarNumber1" prefWidth="30.0">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </TextField>
                                       </children>
                                    </HBox>
                                    <Pane prefHeight="30.0">
                                       <children>
                                          <Label alignment="BASELINE_LEFT" text="%choose_super_stars">
                                             <font>
                                                <Font name="System Italic" size="12.0" />
                                             </font>
                                             <padding>
                                                <Insets left="100.0" top="20.0" />
                                             </padding>
                                          </Label>
                                       </children>
                                    </Pane>
                                    <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
                                       <children>
                                          <Label text="%super_stars" />
                                          <CheckBox fx:id="superStar1" mnemonicParsing="false" text="SuperStar1">
                                             <HBox.margin>
                                                <Insets left="50.0" />
                                             </HBox.margin>
                                          </CheckBox>
                                          <CheckBox fx:id="superStar2" mnemonicParsing="false" text="SuperStar2">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </CheckBox>
                                          <CheckBox fx:id="superStar3" mnemonicParsing="false" text="SuperStar3">
                                             <HBox.margin>
                                                <Insets left="20.0" />
                                             </HBox.margin>
                                          </CheckBox>
                                       </children>
                                    </HBox>
                                    <Pane prefHeight="30.0">
                                       <children>
                                          <Label alignment="BASELINE_LEFT" text="%validity_duration">
                                             <font>
                                                <Font name="System Italic" size="12.0" />
                                             </font>
                                             <padding>
                                                <Insets left="100.0" top="15.0" />
                                             </padding>
                                          </Label>
                                       </children>
                                    </Pane>
                                    <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
                                       <children>
                                          <Label text="%validity_duration_txt" />
                                          <ChoiceBox fx:id="listValidityDuration" prefWidth="40.0">
                                             <HBox.margin>
                                                <Insets left="40.0" />
                                             </HBox.margin>
                                 
                                          </ChoiceBox>
                                          <Button fx:id="buttonBuyTicket" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#actionBuyTicket" text="%buy_ticket_button">
                                             <HBox.margin>
                                                <Insets left="250.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="%draw_winners_tab">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </Tab>
              </tabs>
            </TabPane>
            <Separator prefWidth="200.0" />
            <Pane prefHeight="60.0">
               <children>
                  <Label fx:id="message" text="%welcome">
                     <padding>
                        <Insets left="20.0" top="10.0" />
                     </padding>
                  </Label>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</FlowPane>
